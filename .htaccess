<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule    ^store/([0-9]+)/?$    index.php?page=store&id=$1    [NC,L]    # Process store requests
RewriteRule    ^list/?$    index.php?page=list    [NC,L]    # Process list item requests
</IfModule>

# END WordPress

